{
  "name": "Cubical Agda Codespace (Fixed)",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "remoteUser": "vscode",
  "postCreateCommand": "bash scripts/verify.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "banacorn.agda-mode",
        "justusadam.language-haskell"
      ],
      "settings": {
        "agda.agdaPath": "/usr/local/bin/agda",
        "files.associations": {
          "*.agda": "agda"
        }
      }
    }
  }
}